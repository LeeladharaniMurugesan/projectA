<?xml version="1.0" encoding="UTF-8"?>
<beans xmlns="http://www.springframework.org/schema/beans"
  xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xmlns:context="http://www.springframework.org/schema/context"
  xsi:schemaLocation="http://www.springframework.org/schema/beans 
  http://www.springframework.org/schema/beans/spring-beans.xsd 
  http://www.springframework.org/schema/context/ 
  http://www.springframework.org/schema/context/spring-context.xsd">
  	<bean id ="emp" class = "com.chainsys.projectA.beans.Employee"/>
  	<bean id ="emp2" class = "com.chainsys.projectA.beans.Employee">
  		<property name="id" value="3"></property>
  		<property name="name" value="Dhoni"></property>
  	</bean>  
  	<bean id ="c1" class = "com.chainsys.projectA.beans.Customer" lazy-init="true"/>  
  	<bean id ="actor" class = "com.chainsys.projectA.beans.Actor" scope="prototype"/> 
  	<bean id ="film" class = "com.chainsys.projectA.beans.Movie"> 
  		<constructor-arg type ="java.lang.String" value = "vikram" />
  		<constructor-arg type ="java.lang.String" value = "Logesh" /></bean>
  	<bean id ="scoreboard" class = "com.chainsys.projectA.beans.Scoreboard" factory-method = "createObject" scope="prototype"/>
  	<bean id ="scoreboard1" class = "com.chainsys.projectA.beans.Scoreboard" factory-method = "createObject" scope="prototype">
  	<constructor-arg type ="int" value = "340" />
  	</bean>
  	<bean id ="calendar" class = "com.chainsys.projectA.beans.Calendar" factory-method ="createCalendar" factory-bean="cfactory"/> 
  	<bean id ="cfactory" class = "com.chainsys.projectA.beans.CalendarFactory" /> 
  	<bean id ="appointments" class = "com.chainsys.projectA.beans.Appointments" factory-method ="createAppointments" factory-bean="cfactory"/> 
  	</beans>
  	 